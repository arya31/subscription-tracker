generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id        String   @id @default(uuid())
  name      String
  amount    Float
  startDate DateTime
  isActive  Boolean
  frequency String
  createdAt DateTime @default(now())
}
